<footer class="main-footer">
	<p class="copyright">{{ siteName }} &copy; Copyright 2010-{{ now.year }}.</p>
	<div class="info">
		<ul class="social">
		{% for social in about.social %}
			{% if social.enabled == true %}
			<li>
				<a class="icon icon-{{ social.name }} {{ social.name }}-button" href="{{ social.url }}" target="_blank"></a>
			</li>
			{% endif %}
		{% endfor %}
		</ul>
	</div>
</footer>